<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.predict.tide.black.tide">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".tideCode.application.TideApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".tideCode.module.loginActivity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".tideCode.module.mainActivity.MainActivity" />
        <activity android:name=".tideCode.module.dataAnalysisActivity.DataAnalysisActivity" />
        <activity android:name=".tideCode.module.dateSelectorActivity.DateSelectorActivity" />
        <activity android:name=".tideCode.module.uploadFileActivity.UploadFileActivity" />
        <activity android:name=".tideCode.module.circleFriend.CircleFriendAvtivity" />

        <!-- &lt;!&ndash; 支持横竖屏切换 &ndash;&gt; -->
        <!-- <activity -->
        <!-- android:name=".tideCode.module.publishCircleFriend.PublishAvtivity" -->
        <!-- android:configChanges="orientation|keyboardHidden|screenSize"></activity> -->
        <!-- <activity android:name=".tideCode.module.publishCircleFriMyPreviewActivityvity"></activity> -->

        //图片选择Activity
        <activity
            android:name="com.donkingliang.imageselector.ImageSelectorActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        //图片预览Activity
        <activity
            android:name="com.donkingliang.imageselector.PreviewActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        //图片剪切Activity
        <activity
            android:name="com.donkingliang.imageselector.ClipImageActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".tideCode.module.publishCircleFriend.PublishAvtivity" />
        <activity android:name=".tideCode.module.circleFriend.ImageViewActivity" />
        <activity android:name=".tideCode.module.circleType.CircleTypeActivity" />
        <activity android:name=".tideCode.module.ListTideLocalActivity" />
        <activity android:name=".tideCode.module.ListItemActivity" />
        <activity android:name=".tideCode.module.mainActivity.ShowDateActivity"></activity>
    </application>

</manifest>